<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="admin/tool/wsmanager/db" VERSION="20251211" COMMENT="XMLDB file for Moodle admin/tool/wsmanager"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="tool_wsmanager_functions" COMMENT="Cached information about external functions">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="200" NOTNULL="true" SEQUENCE="false" COMMENT="External function name"/>
        <FIELD NAME="description" TYPE="char" LENGTH="1333" NOTNULL="true" SEQUENCE="false" COMMENT="Description of the web service from the respective db/services.php file"/>
        <FIELD NAME="updated_description" TYPE="char" LENGTH="1333" NOTNULL="false" SEQUENCE="false" COMMENT="Updated description"/>
        <FIELD NAME="parameters_desc" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="JSON schema of the WS function parameters"/>
        <FIELD NAME="returns_desc" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="JSON schema of the return values"/>
        <FIELD NAME="allowed_from_ajax" TYPE="int" LENGTH="2" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="type" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="loginrequired" TYPE="int" LENGTH="2" NOTNULL="true" DEFAULT="1" SEQUENCE="false"/>
        <FIELD NAME="readonlysession" TYPE="int" LENGTH="2" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="all_texts" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="name" UNIQUE="true" FIELDS="name"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
